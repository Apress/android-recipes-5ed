<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Pressed text color -->
    <item android:state_pressed="true" android:color="@color/primaryDarkBlue"/>
    <!-- Checked text color -->
    <item android:state_checked="true" android:color="@color/primaryBlue"/>
    <!-- Default text color -->
    <item android:color="#333"/>
</selector>
